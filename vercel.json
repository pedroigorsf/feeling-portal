{
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "rewrites": [
        {
            "source": "/(css|js|img)/(.*)",
            "destination": "/$1/$2"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ]
}